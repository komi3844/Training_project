<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">

    <changeSet id="CreateTableFaculty" author="Andrey">
        <createTable tableName="faculty">
            <column name="idfaculty" type="BIGINT">
                <constraints foreignKeyName="faculty_idfaculty_student" references="student(idpfaculty)"/>
            </column>
            <column name="facultyname" type="VARCHAR(50)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>